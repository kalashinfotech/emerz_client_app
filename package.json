{
  "name": "emerz_client_app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "new:schema": "node scripts/create-schema.ts"
  },
  "dependencies": {
    "@origin-space/image-cropper": "^0.1.9",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-form": "^1.23.0",
    "@tanstack/react-form-devtools": "^0.1.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.131.50",
    "@tanstack/react-router-devtools": "^1.131.50",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-plugin": "^1.131.50",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "emblor": "^1.4.8",
    "framer-motion": "^12.23.19",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.544.0",
    "radix-ui": "^1.4.3",
    "react": "^19.1.1",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^3.2.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@tanstack/eslint-config": "^0.3.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 122,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "trailingComma": "all",
    "importOrder": [
      "^react",
      "<THIRD_PARTY_MODULES>",
      "^@/types(.*)$",
      "^@/components/ui/(.*)$",
      "^@/components/(.*)$",
      "^@/pages/(.*)$",
      "^@/api/(.*)$",
      "^@/hooks/(.*)$",
      "^@/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ]
  },
  "overrides": {
    "emblor": {
      "react": "^19.1.1",
      "react-dom": "^19.1.1"
    }
  }
}
